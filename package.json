{
  "name": "hello-world",
  "displayName": "hello-world",
  "description": "practice",
  "version": "1.0.0",
  "publisher": "Marxu",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onCommand:extension.sayHello"],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.sayHello",
        "title": "Hello World"
      }
    ],
    "keybindings": [
        {
            "command": "extension.sayHello",
            "key": "ctrl+f10",
            "mac": "cmd+f10",
            "when": "editorTextFocus"
        }
    ],
    "menus": {
        "editor/context": [
            {
                "when": "editorFocus",
                "command": "extension.sayHello",
                "group": "navigation"
            }
        ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  }
}
